
[{{- range $index, $page := .Site.Pages -}}
  {{- if ne $page.Type "json" -}}
    {{ $page_content := $page.Content }} 
    {{$page_content := replaceRE "(<h)(.|\n)*?" "<end>$1" $page_content}}
    {{- range $index, $html :=  findRE "<h(.|\n)*?<end>" $page_content -}}  
      {{ $html_id := findRE "id=\"(.|\n)*?\"" . }} 
      {{ $html_id_f := delimit $html_id "" ""}}
      {{ $html_id_f }}
      {{ $html_id_f := substr $html_id_f 4 0 }}
      {{ $html_id_f  }}
    {{- end -}}
  {{- end -}}
{{- end -}}
  {
    "objectID": "{{ md5 "null" }}",
    "url": "",
    "title": "null",
    "content": "null",
    "hierarchy": { 
      "lvl0": "null",
      "lvl1": "",
      "lvl2": "",
      "lvl3": "",
      "lvl4": "",
      "lvl5": "",
      "lvl6": ""
    }
  }
]


