
[{{- range $index, $page := .Site.Pages -}}
  {{- if ne $page.Type "json" -}}
    {{$page_content := replaceRE "(.|\n)*?" "<content-start>$1<content-end>" $page.Content}}
    {{$page_content := replaceRE "(<h)(.|\n)*?" "<heading-end>$1" $page_content}}
    {{ $page_content }}

    {{- range $index, $html :=  findRE "<h(.|\n)*?<heading-end>" $page_content -}}  
      {{- $html_id := findRE "id=(.|\n)*?>" . -}} 
      {{ $html_id_f := delimit $html_id "" ""}}
      {{ $html_id_f := replace $html_id_f "id=\"" ""}}
      {{ $html_id_f := replace $html_id_f "\">" ""}}


    {{- end -}}
  {{- end -}}
{{- end -}}
  {
    "objectID": "{{ md5 "null" }}",
    "url": "",
    "title": "null",
    "content": "null",
    "hierarchy": { 
      "lvl0": "null",
      "lvl1": "",
      "lvl2": "",
      "lvl3": "",
      "lvl4": "",
      "lvl5": "",
      "lvl6": ""
    }
  }
]


